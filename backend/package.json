  "dependencies": {
    "@circle-fin/circle-sdk": "^1.3.0",
    "@google-cloud/functions-framework": "^3.1.3",
    "@project-serum/anchor": "^0.26.0",
    "uuid": "^9.0.0",
    "yamlenv": "^1.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.1",
    "typescript": "^4.9.4"
  }
}
